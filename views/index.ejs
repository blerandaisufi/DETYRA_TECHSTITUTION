<% include partials/header %>


<div class="container">



<h3>Kontratat</h3>


<table class="table">
  <thead>
    <tr>
       <th scope="col">Nr. rendor i prokurimit </th>
      <th scope="col">Lloji i prokurimit </th>
	  <th scope="col">Aktiviteti i prokurimit </th>
	  <th scope="col">Data e inicimit te aktivitetit te prokurimit</th>
